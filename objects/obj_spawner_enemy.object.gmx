      {
         if (global.monies_enemy &gt;= obj_btn_lenny.cost)
         {
            instance_create( x, y, obj_lenny_bad );
            global.monies_enemy -= obj_btn_lenny.cost;
            
            nextspawn = "";
         }
      }
      
      if ( nextspawn == "jose" )
